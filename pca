
from sklearn.datasets import load_breast_cancer
from sklearn.preprocessing import StandardScaler
from sklearn.decomposition import PCA
from sklearn.model_selection import train_test_split
import tensorflow
from tensorflow import keras
from keras.layers import Dense
from keras import Sequential

 
x,y = load_breast_cancer(return_X_y=True)

scaler = StandardScaler().fit_transform(x)

pca2 = PCA(n_components=2).fit_transform(scaler)

x_train,x_test,y_train,y_test = train_test_split(pca2,y,test_size=0.2,random_state=42)

model = Sequential()
model.add(Dense(16,activation='relu',input_dim=(2)))  #kernel_regularizer=regularizers.l2(0.01)
model.add(Dense(1,activation='sigmoid'))
model.compile(optimizer='adam',loss='binary_crossentropy' ,metrics=['accuracy'])
model.fit(x_train,y_train,epochs=5)
